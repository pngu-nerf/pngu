{
  "name": "pngu",
  "compatibility_date": "2026-01-07",
  "compatibility_flags": ["nodejs_compat"],
  "main": "./dist/_worker.js/index.js",
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS"
  },
  "vars": {
    "SITE_URL": "https://pngu.info"
  },
  "r2_buckets": [
    {
      "binding": "BUCKET",
      "bucket_name": "pngu-assets"
    }
  ]
}
