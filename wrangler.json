{
  "name": "pngu",
  "compatibility_date": "2025-10-08",
  "compatibility_flags": ["nodejs_compat"],
  "main": "./dist/_worker.js/index.js",
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS"
  },
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true,
  
  "routes": [
    {
      "pattern": "pngu.info/*",
      "zone_name": "pngu.info"
    }
  ],
  
  "vars": {
    "SITE_URL": "https://pngu.info"
  }
}
