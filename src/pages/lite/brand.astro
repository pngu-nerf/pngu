---
import { getR2Data } from '../../lib/r2';
import { R2_BRAND_BASE } from '../../constants';
const raw = await getR2Data("PNGU-Brand-Assets-main/");
const assets = raw[0]?.files || []; 
---
<!doctype html><meta charset="utf-8"><meta name="viewport" content="width=device-width,initial-scale=1"><link rel="icon" href="data:;base64,iVBOR="><link rel="preconnect" href="https://assets.pngu.info"><title>Brand | Lite</title><style>body{font:13px sans-serif;background:#333;color:#fff;text-align:center;padding:5vh 1rem}b{display:block;font-size:11px;color:#888;margin-bottom:2rem;text-transform:uppercase}a{color:#fff;text-decoration:none;display:block;padding:12px 0;width:240px;margin:0 auto;border-bottom:1px solid #444}a:active{background:#444}.reg{display:block;color:#555;font-size:11px;text-decoration:underline;margin-bottom:1rem}.main-site{margin-bottom:3rem}</style><b>BRAND ASSETS</b><a href="/" class="reg">Back to home</a><a href="https://pngu.info/brand" class="reg main-site">Back to regular site</a>{assets.map((f:string)=>(<a href={`${R2_BRAND_BASE}/${f}`} download={f}>{f.replace('.png','').replace(/-/g,' ')}</a>))}
